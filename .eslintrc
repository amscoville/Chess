{
    "extends": "airbnb",
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": 0,
        "no-plusplus": 0,
        "no-use-before-define": 0,
        "comma-dangle": 0,
        "class-methods-use-this": 0,
        "no-continue": 0
    },
    "env": {
        "browser": "true"
    }
}
